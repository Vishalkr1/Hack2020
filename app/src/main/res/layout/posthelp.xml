<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0B1239">


    <Button
        android:id="@+id/emergency2"
        android:layout_width="173dp"
        android:layout_height="88dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/btn_custom"
        android:onClick="onDialEmergency"
        android:text="EMERGENCY\n100"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ambulance"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emergency3"
        app:layout_constraintVertical_bias="0.284"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/emergency3"
        android:layout_width="149dp"
        android:layout_height="71dp"
        android:layout_marginStart="49dp"
        android:layout_marginLeft="49dp"
        android:layout_marginEnd="49dp"
        android:layout_marginRight="49dp"
        android:layout_marginBottom="80dp"
        android:background="@drawable/btn_custom"
        android:onClick="onDialemergency3"
        android:text="Call Care Taker"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.633"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/covid19help"
        android:layout_width="129dp"
        android:layout_height="70dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:background="@drawable/btn_custom"
        android:onClick="onDialPoison"
        android:text="Covid19 Helpline\n1075"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toTopOf="@+id/emergency3"
        app:layout_constraintEnd_toStartOf="@+id/emergency"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/ambulance"
        android:layout_width="173dp"
        android:layout_height="88dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/btn_custom"
        android:onClick="onDialEmergencyManagement"
        android:text="Ambulance\n108"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/emergency2"
        app:layout_constraintTop_toBottomOf="@+id/emergency3"
        app:layout_constraintVertical_bias="0.468"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/emergency"
        android:layout_width="129dp"
        android:layout_height="70dp"
        android:layout_marginStart="49dp"
        android:layout_marginLeft="49dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        android:layout_marginBottom="81dp"
        android:background="@drawable/btn_custom"
        android:onClick="onDialemergency2"
        android:text="EMERGENCY\n100"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toTopOf="@+id/emergency3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/covid19help"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>