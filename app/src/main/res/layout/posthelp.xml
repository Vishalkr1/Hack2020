<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#151515"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:text="Help Desk"
        android:textAlignment="center"
        android:padding="30dp"
        android:textColor="#FFFFFF"
        android:textSize="40sp"
        android:fontFamily="sans-serif-smallcaps"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="61dp"
        android:gravity="center_horizontal" />
    <LinearLayout
        android:layout_marginTop="50dp"
        android:layout_width="match_parent"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="35dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/doctor"
            android:layout_width="150dp"
            android:layout_marginRight="47dp"
            android:layout_height="132dp"
            android:background="@drawable/six"
            android:onClick="onDialEmergency2"
            android:textColor="#ffffff"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="348dp" />

        <Button
            android:id="@+id/covid19help"
            android:layout_width="150dp"
            android:layout_height="132dp"
            android:background="@drawable/one"
            android:onClick="onDialPoison"
            android:textColor="#ffffff"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="154dp" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:orientation="horizontal">
        <Button
            android:id="@+id/ambulance"
            android:layout_width="150dp"
            android:layout_height="132dp"
            android:layout_marginRight="47dp"
            android:background="@drawable/five"
            android:onClick="onDialEmergencyManagement"
            android:textColor="#ffffff"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="256dp"
            tools:layout_editor_absoluteY="355dp" />

        <Button
            android:id="@+id/emergency"
            android:layout_width="150dp"
            android:layout_height="132dp"
            android:background="@drawable/seven"
            android:onClick="onDialEmergency"
            android:textColor="#ffffff"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="224dp"
            tools:layout_editor_absoluteY="154dp" />

    </LinearLayout>


    <Button
        android:id="@+id/emergency3"
        android:layout_width="150dp"
        android:layout_height="132dp"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="130dp"
        android:layout_marginRight="131dp"
        android:background="@drawable/taker2"
        android:onClick="onDialemergency3"
        android:textColor="#ffffff"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="528dp" />


</LinearLayout>
